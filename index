    <div class="back"
         id="back">
        <img src="ArtAllNight/back.jpg"
             id="backImg"
             style="
             width: 100%;
             height: 100%;
             opacity: 100%;
             visibility: hidden;
             " />
        <script>
            document.getElementById("back").addEventListener("click", backClick);
            function backClick() {
                var image = document.getElementById('bgDivImg');
                if (image.src.match("ArtAllNight/2.jpg")) {
                    image.src = "ArtAllNight/1.jpg";
                    document.getElementById("backImg").style.visibility = "hidden";
                }
                else if (image.src.match("ArtAllNight/3.jpg")) {
                    image.src = "ArtAllNight/2.jpg";
                }
                else if (image.src.match("ArtAllNight/4b.jpg")) {
                    image.src = "ArtAllNight/3.jpg";
                }
                else if (image.src.match("ArtAllNight/6.jpg")) {
                    image.src = "ArtAllNight/4b.jpg";
                }
                else if (image.src.match("ArtAllNight/7.jpg")) {
                    image.src = "ArtAllNight/6.jpg";
                }
                else if (image.src.match("ArtAllNight/8.jpg")) {
                    image.src = "ArtAllNight/7.jpg";
                }
                else if (image.src.match("ArtAllNight/9.jpg")) {
                    image.src = "ArtAllNight/8.jpg";
                }
                else if (image.src.match("ArtAllNight/10.jpg")) {
                    image.src = "ArtAllNight/9.jpg";
                }
                else if (image.src.match("ArtAllNight/11.jpg")) {
                    image.src = "ArtAllNight/10.jpg";
                }
                else if (image.src.match("ArtAllNight/12.jpg")) {
                    image.src = "ArtAllNight/11.jpg";
                }
                else if (image.src.match("ArtAllNight/13.jpg")) {
                    image.src = "ArtAllNight/12.jpg";
                }
                else if (image.src.match("ArtAllNight/14.jpg")) {
                    image.src = "ArtAllNight/13.jpg";
                    document.getElementById("nextImg").style.visibility = "visible";
                    document.getElementById("links").style.visibility = "hidden";
                    document.getElementById("links2").style.visibility = "hidden";
                }

            }


        </script>


    </div>

    <div class="next"
         id="next">
        <img src="ArtAllNight/next.jpg"
             id="nextImg"
             style="
             width: 100%;
             height: 100%;
             opacity: 100%;
             " />
        <script>
            document.getElementById("next").addEventListener("click", nextClick);
            function nextClick() {
                var image = document.getElementById('bgDivImg');
                if (image.src.match("ArtAllNight/1.jpg")) {
                    image.src = "ArtAllNight/2.jpg";
                    document.getElementById("backImg").style.visibility = "visible";
                }
                else if (image.src.match("ArtAllNight/2.jpg")) {
                    image.src = "ArtAllNight/3.jpg";
                }
                else if (image.src.match("ArtAllNight/3.jpg")) {
                    image.src = "ArtAllNight/4b.jpg";
                }
                else if (image.src.match("ArtAllNight/4b.jpg")) {
                    image.src = "ArtAllNight/6.jpg";
                }
                else if (image.src.match("ArtAllNight/6.jpg")) {
                    image.src = "ArtAllNight/7.jpg";
                }
                else if (image.src.match("ArtAllNight/7.jpg")) {
                    image.src = "ArtAllNight/8.jpg";
                }
                else if (image.src.match("ArtAllNight/8.jpg")) {
                    image.src = "ArtAllNight/9.jpg";
                }
                else if (image.src.match("ArtAllNight/9.jpg")) {
                    image.src = "ArtAllNight/10.jpg";
                }
                else if (image.src.match("ArtAllNight/10.jpg")) {
                    image.src = "ArtAllNight/11.jpg";
                }
                else if (image.src.match("ArtAllNight/11.jpg")) {
                    image.src = "ArtAllNight/12.jpg";
                }
                else if (image.src.match("ArtAllNight/12.jpg")) {
                    image.src = "ArtAllNight/13.jpg";
                }
                else if (image.src.match("ArtAllNight/13.jpg")) {
                    image.src = "ArtAllNight/14.jpg";
                }
                else if (image.src.match("ArtAllNight/14.jpg")) {
                    document.getElementById("nextImg").style.visibility = "hidden";
                    document.getElementById("links").style.visibility = "visible";
                    document.getElementById("links2").style.visibility = "visible";
                }

